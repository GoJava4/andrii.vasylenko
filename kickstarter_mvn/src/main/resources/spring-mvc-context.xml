<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
        http://www.springframework.org/schema/mvc
        http://www.springframework.org/schema/mvc/spring-mvc-4.1.xsd">

	<mvc:annotation-driven />

	<bean id="quoteController"
		class="ua.kiev.avp256.kickstarter_server.controller.QuoteController"
		autowire="byName" />

	<bean id="categoryController"
		class="ua.kiev.avp256.kickstarter_server.controller.CategoryController"
		autowire="byName" />

	<bean id="projectController"
		class="ua.kiev.avp256.kickstarter_server.controller.ProjectController"
		autowire="byName" />

	<bean id="questionController"
		class="ua.kiev.avp256.kickstarter_server.controller.QuestionController"
		autowire="byName" />

	<bean id="paymentController"
		class="ua.kiev.avp256.kickstarter_server.controller.PaymentController"
		autowire="byName" />

	<bean id="paymentVariantController"
		class="ua.kiev.avp256.kickstarter_server.controller.PaymentVariantController"
		autowire="byName" />
</beans>