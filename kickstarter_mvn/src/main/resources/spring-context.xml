<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<import resource="classpath:hibernate-context.xml" />

	<bean id="quoteService" class="kickstarter.service.QuoteService"
		autowire="byName" />
	<bean id="categoryService" class="kickstarter.service.CategoryService"
		autowire="byName" />
	<bean id="projectService" class="kickstarter.service.ProjectService"
		autowire="byName" />
	<bean id="questionService" class="kickstarter.service.QuestionService"
		autowire="byName" />
	<bean id="paymentService" class="kickstarter.service.PaymentService"
		autowire="byName" />
</beans>